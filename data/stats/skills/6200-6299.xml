	<skill id="6209" levels="4" name="Weapon Grade Penalty" >
		<!--
		The equipped weapon's grade doesn't match your level, so the following penalties are incurred:
		Decrease in accuracy by 16, Critical Rate by 10-40%, Critical Power by 10-40%, Atk. Spd. by 10%, and P. Atk. by 10%.
		-->
		<table name="#power">10 20 30 40</table>
		<table name="#failPower">-900 -1400 -1800 -2300 </table> 
		<set name="target" val="TARGET_SELF"/>
		<set name="skillType" val="DEBUFF"/>
		<set name="operateType" val="OP_PASSIVE"/>
		<for>
			<sub order="0x80" stat="p_hit" val="16"/>
			<sub order="0x80" stat="p_critical_rate_per" val="#power"/>
			<sub order="0x80" stat="p_critical_damage_per" val="#power"/>
			<add order="0x40" stat="magicPower" val="#failPower" /> 
			<mul order="0x70" stat="p_attack_speed" val="0.9"/>
			<mul order="0x70" stat="p_physical_attack" val="0.9"/>
		</for>
	</skill>
	<skill id="6213" levels="4" name="Armor Grade Penalty" >
		<!--
		When equipped with armor or accessories whose grad doesn't fit with your level, a penalty is incurred ith
		decreases in evasion by 2.5-10, Atk. Spd./Casting Spd./moving speed by 16.67-51.77%.
		-->
		<table name="#p_avoid">2.5 5 7.5 10</table>
		<table name="#rate">0.8333 0.6944 0.5787 0.4823</table>
		<set name="target" val="TARGET_SELF"/>
		<set name="skillType" val="DEBUFF"/>
		<set name="operateType" val="OP_PASSIVE"/>
		<for>
			<sub order="0x80" stat="p_avoid" val="#p_avoid"/>
			<mul order="0x70" stat="p_attack_speed" val="#rate"/>
			<mul order="0x70" stat="p_magic_speed" val="#rate"/>
			<mul order="0x70" stat="p_speed" val="#rate"/>
		</for>
	</skill>
