	<skill id="3204" levels="3" name="Item Skill: Reflect Damage">
		<!--
			Done by:	Diamond
			Last modify:	29.12.2009, SYS
			Description:
			Lvl 1-3 - Active: Allows you to reflect some of the damage you incurred back to the enemy for a certain amount of time. Excludes damage from skill or remote attacks.
		-->
		<table name="#reflectDam">10 15 20</table>
		<set name="target" val="TARGET_SELF"/>
		<set name="skillType" val="BUFF"/>
		<set name="operateType" val="OP_ACTIVE"/>
		<for>
			<effect name="Buff" stackOrder="10" stackType="augment" time="300">
				<add order="0x40" stat="reflectDam" val="#reflectDam"/>
			</effect>
		</for>
	</skill>
