<list>
	<skill id="3200" levels="3" name="Item Skill: Spell Refresh">
		<!--
			Description:
			Active: Temporarily decreases the re-use time for magic skills.
		-->
		<set name="target" val="TARGET_SELF"/>
		<set name="skillType" val="BUFF"/>
		<set name="operateType" val="OP_ACTIVE"/>
		<for>
			<effect name="Buff" time="60" stackOrder="10" stackType="augment">
				<mul order="0x30" stat="mReuse" val="0.85"/>
			</effect>
		</for>
	</skill>
