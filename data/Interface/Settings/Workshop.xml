	<!-- [RU] Прочие параметры энчанта -->
	<!-- [EN] Another enchant settings -->
	<interface_setting name="WorkshopEnchWeaponMax" type="TYPE_INT" value="16"/>
	<interface_setting name="WorkshopEnchArmorMax" type="TYPE_INT" value="12"/>
	
	<!-- [RU] Включить/выключить энчант по очереди (+1 на каждый выбранный предмет) -->
	<!-- [EN] Enable/disable enchant in turn (+1 for each selected item) -->
	<interface_setting name="WorkshopEnchInTurnEnable" type="TYPE_CHAR" value="1"/>
	
	<!-- [RU] Количество предметов, которое можно выбрать для энчанта (не меньше 1 и не более 240) -->
	<!-- [EN] The number of items that can be selected for the enchant (no less than 1 and no more than 240) -->
	<interface_setting name="WorkshopEnchMaxItemNum" type="TYPE_INT" value="240"/>
	
	<!-- [RU] Значение на которые сбрасывается энчант при использовании bless свитков, обязательно должно соответствовать настройкам сервера! -->
	<!-- [EN] Value to which the enchant is reset when using the bless scrolls, it must necessarily match the server settings! -->
	<interface_setting name="WorkshopEnchBlessFail" type="TYPE_INT" value="0"/>
	
	<!-- [RU] Минимальная задержка и значение на которое будет увеличиваться на каждое положение бегунка (в мс, 1 секунда = 1000 мс) -->
	<!-- [EN] The minimum delay and the value that will increase for each position of the slider (in ms, 1 second = 1000 ms) -->
	<interface_setting name="WorkshopEnchMinDelay" type="TYPE_INT" value="2500"/>
	<interface_setting name="WorkshopEnchDelayRange" type="TYPE_INT" value="100"/>
	<interface_setting name="WorkshopLsMinDelay" type="TYPE_INT" value="2500"/>
	<interface_setting name="WorkshopLsDelayRange" type="TYPE_INT" value="100"/>
	<interface_setting name="WorkshopAttMinDelay" type="TYPE_INT" value="1000"/>
	<interface_setting name="WorkshopAttDelayRange" type="TYPE_INT" value="100"/>
	
	<!-- [RU] Enable: Включить/выключить кнопку помощи (включить - 1, выключить - 0) -->
	<!-- [EN] Enable: Enable/Disable help button (enable - 1, disable - 0) -->
	<!-- [RU] Type: 0 - открыть html клиента, 1 - открыть ссылку в браузере -->
	<!-- [EN] Type: 0 - show client html, 1 - open URL -->
	<!-- [RU] ActionRu: для Type 0 - название html клиента на русском, для Type 1 - ссылка -->
	<!-- [EN] ActionRu: for Type 0 - name client html on russian, for Type 1 - URL -->
	<!-- [RU] ActionEn: для Type 0 - название html клиента на английском, для Type 1 - ссылка -->
	<!-- [EN] ActionEn: for Type 0 - name client html on english, for Type 1 - URL -->
	<interface_setting name="WorkshopEnchHelpBtnEnable" type="TYPE_CHAR" value="1"/>
	<interface_setting name="WorkshopEnchHelpBtnType" type="TYPE_INT" value="0"/>
	<interface_setting name="WorkshopEnchHelpBtnActionRu" type="TYPE_TEXT" value="EnchHelp-ru.htm"/>
	<interface_setting name="WorkshopEnchHelpBtnActionEn" type="TYPE_TEXT" value="EnchHelp-en.htm"/>
	
