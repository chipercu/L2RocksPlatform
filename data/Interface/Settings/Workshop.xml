	<interface_setting name="WorkshopEnchScrollsArmor5" type="TYPE_ARR_INT" value="960;962;20518;20520;22222;17264;22315;32416;22429"/>
	
	<!-- [RU] Список благославенных свитков по грейдам для брони (1 - D, 2 - C, 3 - B, 4 - A, 5 - S) -->
	<!-- [EN] Blessed Scrolls list by grade for armor (1 - D, 2 - C, 3 - B, 4 - A, 5 - S) -->
	<interface_setting name="WorkshopEnchBlessScrollsArmor1" type="TYPE_ARR_INT" value="6576"/>
