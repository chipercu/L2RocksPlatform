	<interface_setting name="WorkshopEnchScrollsWeapon2" type="TYPE_ARR_INT" value="951;953;22007;22227;17259;22341;15349"/>
	<interface_setting name="WorkshopEnchScrollsWeapon3" type="TYPE_ARR_INT" value="947;949;22008;22014;22018;22225;17257;22317;15348"/>
