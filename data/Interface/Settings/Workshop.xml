	<interface_setting name="WorkshopLsHelpBtnActionEn" type="TYPE_TEXT" value="LifeStoneHelp-en.htm"/>
	
	<interface_setting name="WorkshopAttHelpBtnEnable" type="TYPE_CHAR" value="1"/>
	<interface_setting name="WorkshopAttHelpBtnType" type="TYPE_INT" value="0"/>
	<interface_setting name="WorkshopAttHelpBtnActionRu" type="TYPE_TEXT" value="AttHelp-ru.htm"/>
	<interface_setting name="WorkshopAttHelpBtnActionEn" type="TYPE_TEXT" value="AttHelp-en.htm"/>
	
