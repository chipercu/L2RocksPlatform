	<interface_setting name="TypeSlot1" type="TYPE_INT" value="2"/>
	<!-- [RU] Включить/выключить слот 1 -->
	<!-- [EN] Enable/Disable slot 1 -->
	<interface_setting name="ShowSlot1" type="TYPE_CHAR" value="1"/>


