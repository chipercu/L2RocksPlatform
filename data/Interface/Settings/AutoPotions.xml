<?xml version='1.0' encoding='utf-8'?>
<list>
	<!-- [RU] Настройки авто-потионов -->
	<!-- [EN] Auto potions settings -->
	<!-- [RU] 0 - выключено для всех (отключает все нижеперечисленные опции, интерфейс будет работать по дефолту) -->
	<!-- [EN] 0 - disabled for all (disables all of the following options, the interface will work by default) -->
	<!-- [RU] 1 - включено только для игроков с ПА -->
	<!-- [EN] 1 - enabled only for PA players -->
	<!-- [RU] 2 - включено для всех -->
	<!-- [EN] 2 - enabled for all -->
	<!-- [RU] Для отключения слотов с типом 4 (банки душ, для Камаэлей, тип так же следует поменять на 0 (NONE)) -->
	<!-- [EN] To disable slots with type 4 (Soul Potions, for Kamaels, the type should also be changed to 0 (NONE)) -->
	<interface_setting name="AutoPotionsMode" type="TYPE_CHAR" value="0"/>
	
	<!-- [RU] Айди предмета и задержка (в мс, 1 секунда = 1000 мс) для слота 0 -->
	<!-- [EN] ItemID and delay (in ms, 1 second = 1000 ms) for slot 0 -->
	<interface_setting name="ItemIdSlot0" type="TYPE_ARR_INT" value="10410"/>
	<interface_setting name="DelaySlot0" type="TYPE_ARR_INT" value="0"/>
	<!-- [RU] Фоновая текстура для слота 0 -->
	<!-- [EN] Background texture for slot 0 -->
	<interface_setting name="BackTextureSlot0" type="TYPE_TEXT" value="L2UI_CT2.AutoPotionsWnd.Souls"/>
	<!-- [RU] Тип слота 0: 0 - NONE, 1 - CP, 2 - HP, 3 - MP, 4 - Soul Potions (Для 4 типа задержка на использование игнорируется) -->
	<!-- [EN] Type slot 0: 0 - NONE, 1 - CP, 2 - HP, 3 - MP, 4 - Soul Potions (For type 4, the usage delay is ignored) -->
	<interface_setting name="TypeSlot0" type="TYPE_INT" value="4"/>
	<!-- [RU] Включить/выключить слот 0 -->
	<!-- [EN] Enable/Disable slot 0 -->
	<interface_setting name="ShowSlot0" type="TYPE_CHAR" value="1"/>


	<!-- [RU] Айди предмета и задержка (в мс, 1 секунда = 1000 мс) для слота 1 -->
	<!-- [EN] ItemID and delay (in ms, 1 second = 1000 ms) for slot 1 -->
	<interface_setting name="ItemIdSlot1" type="TYPE_ARR_INT" value="8627"/>
